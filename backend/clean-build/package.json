{
  "name": "ai-browser-clean",
  "version": "1.0.0",
  "description": "Standalone AI-powered browser application",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "build:web": "webpack --mode production",
    "dev:web": "webpack serve --mode development",
    "test": "jest",
    "test:ai": "jest src/ai/",
    "test:browser": "jest src/browser/",
    "test:privacy": "jest src/privacy/",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "docker:build": "docker build -t ai-browser .",
    "docker:run": "docker run -p 3000:3000 ai-browser",
    "docker:push": "docker push your-registry/ai-browser"
  },
  "build": {
    "appId": "com.aibrowser.clean",
    "productName": "AI Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "public/**/*",
      "package.json",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icons/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icons/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icons/icon.png"
    }
  },
  "keywords": [
    "ai",
    "browser",
    "contextual-search",
    "privacy",
    "video-analysis",
    "content-aggregation",
    "desktop",
    "electron"
  ],
  "author": "AI Browser Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "@types/node": "^20.10.5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "axios": "^1.6.2",
    "sharp": "^0.33.2",
    "puppeteer-core": "^21.6.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}